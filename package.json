{
  "name": "ev-db",
  "version": "1.0.0",
  "description": "A comprehensive, community-driven platform for electric vehicle data management with contribution workflows, image galleries, and API access.",
  "main": "index.js",
  "scripts": {
    "dev:frontend": "pnpm --filter frontend dev",
    "dev:backend": "pnpm --filter backend dev",
    "build": "pnpm --filter frontend build",
    "test:backend": "pnpm --filter backend vitest run",
    "test:frontend": "pnpm --filter frontend vitest run",
    "test:all": "pnpm run test:backend && pnpm run test:frontend"
  },
  "keywords": ["electric-vehicles", "ev", "database", "react", "typescript", "hono", "daisyui"],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.11"
  }
}
